<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.afpa.AjoutController">
   <children>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" spacing="50.0">
         <children>
            <HBox spacing="50.0">
               <children>
                  <Label text="Nom" />
                  <TextField fx:id="nomField" />
               </children>
            </HBox>
            <HBox spacing="35.0">
               <children>
                  <Label text="Prenom" />
                  <TextField fx:id="prenomField" />
               </children>
            </HBox>
            <HBox spacing="45.0">
               <children>
                  <Label text="Offre" />
                  <TextField fx:id="offreField" />
               </children>
            </HBox>
            <HBox layoutX="30.0" layoutY="269.0" spacing="50.0">
               <children>
                  <Label text="age" />
                  <TextField fx:id="ageField" />
               </children>
            </HBox>
            <Button fx:id="btnAjout" mnemonicParsing="false" onAction="#ajout" text="valider" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
